<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="100.0" prefWidth="300.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gersonberger.AlertController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <Label fx:id="alertLabel" />
   <Region prefHeight="200.0" prefWidth="200.0" />
   <HBox>
      <Region HBox.hgrow="ALWAYS" />
      <Button mnemonicParsing="false" onAction="#handleClose" prefWidth="70.0" text="Close" />
   </HBox>
</VBox>
